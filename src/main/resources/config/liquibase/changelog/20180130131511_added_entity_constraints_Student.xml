<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Student.
    -->
    <changeSet id="20180130131511-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id_id"
                                 baseTableName="student"
                                 constraintName="fk_student_user_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="school_class_id"
                                 baseTableName="student"
                                 constraintName="fk_student_school_class_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school_class"/>

        <addForeignKeyConstraint baseColumnNames="section_id"
                                 baseTableName="student"
                                 constraintName="fk_student_section_id"
                                 referencedColumnNames="id"
                                 referencedTableName="section"/>

        <addForeignKeyConstraint baseColumnNames="session_id"
                                 baseTableName="student"
                                 constraintName="fk_student_session_id"
                                 referencedColumnNames="id"
                                 referencedTableName="session"/>

    </changeSet>
</databaseChangeLog>
